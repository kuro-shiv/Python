   "source": [
    "def factorial(num):\n",
    "    fac = 1\n",
    "    for i in range (1, num + 1):\n",
    "        fac = fac * i\n",
    "    return fac\n",
    "\n",
    "\n",
    "num = input(print(\"enter a number\"))\n",
    "num = int (num)\n",
    "\n",
    "\n",
    "\n",
    "print (factorial(num))"
   ]
